{
  "name": "phaser-first",
  "version": "1.0.0",
  "description": "Breakout game using Phaser with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint src --ext ts",
    "serve": "webpack --config webpack.config.js && ts-node src/server/index.ts",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "watch": "webpack --watch --config webpack.config",
    "node": "ts-node src/server/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackHarpy/phaser-first.git"
  },
  "author": "Maria Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlackHarpy/phaser-first/issues"
  },
  "homepage": "https://github.com/BlackHarpy/phaser-first#readme",
  "dependencies": {
    "express": "^4.16.2",
    "phaser-ce": "^2.9.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.51",
    "eslint": "^4.11.0",
    "expose-loader": "^0.7.3",
    "file-loader": "^1.1.5",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "typescript-eslint-parser": "^9.0.0",
    "webpack": "^3.8.1"
  }
}
